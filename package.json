{
  "name": "helpdesk-system",
  "version": "1.0.0",
  "description": "helpdesk-system",
  "repository": {
    "type": "git",
    "url": "https://github.com/backsty/ahj-homeworks/tree/video/http"
  },
  "main": "./src/index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "webpack serve --config webpack.dev.cjs --mode development",
    "build": "webpack --config webpack.prod.cjs --mode production",
    "watch": "webpack --watch --config webpack.dev.cjs --mode development",
    "server": "node src/backend/server.js",
    "dev": "concurrently \"yarn server\" \"yarn start\"",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --watch --config jest.config.cjs",
    "test:coverage": "jest --coverage --config jest.config.cjs",
    "lint": "yarn lint:js && yarn lint:styles && yarn format:check",
    "lint:js": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:styles": "stylelint \"src/**/*.css\" --fix",
    "format": "prettier --write \"src/**/*.{js,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,css,json}\"",
    "clean": "rimraf dist",
    "preshow:coverage": "yarn test:coverage",
    "show:coverage": "live-server coverage/lcov-report",
    "preshow:dist": "yarn build",
    "show:dist": "http-server dist",
    "predeploy": "yarn lint && yarn test && yarn build",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [],
  "author": "backsty",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@eslint/js": "^9.17.0",
    "@testing-library/dom": "^7.21.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "cypress": "^13.16.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "glob": "^11.0.0",
    "globals": "^15.13.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "http-server": "^14.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "terser-webpack-plugin": "^5.3.11",
    "typescript": "^4.8.4",
    "webpack": "^5.97.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "core-js": "^3.39.0",
    "koa": "^2.15.3",
    "koa-body": "^6.0.1",
    "koa-router": "^13.0.1"
  },
  "homepage": "https://backsty.github.io/ahj-homeworks/http"
}
